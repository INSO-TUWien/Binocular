{
  "name": "binocular-backend",
  "version": "v2.0.0",
  "description": "Time-based visualizations of combined software engineering data",
  "main": "binocular.ts",
  "type": "module",
  "scripts": {
    "test": "npm run test-server && npm run test-ui",
    "test-server": "mocha --config=.mocharc.json --exit --reporter spec test/backend",
    "test-ui": "jest --config jest.config.js",
    "coverage": "npm run coverage-server && npm run coverage-ui && npm run collect-coverage",
    "coverage-server": "nyc mocha --config=.mocharc.json --exit ../test/backend",
    "coverage-ui": "jest --config jest-coverage.config.js",
    "collect-coverage": "../scripts/collect-coverage.js",
    "check": "eslint .",
    "dev": "tsx watch binocular.ts run ../ --no-frontend",
    "fix": "eslint --fix ."
  },
  "bin": {
    "binocular": "index.js"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.8.12",
    "@octokit/core": "^5.0.0",
    "@octokit/graphql": "^7.0.1",
    "@octokit/plugin-paginate-graphql": "^4.0.0",
    "@octokit/rest": "^19.0.7",
    "@types/lodash": "^4.14.199",
    "adm-zip": "^0.5.10",
    "arangojs": "^8.1.0",
    "archiver": "^5.3.1",
    "body-parser": "^1.20.2",
    "buffer": "^6.0.3",
    "chai": "^4.3.7",
    "chalk": "^4.1.2",
    "commander": "^10.0.0",
    "cors": "^2.8.5",
    "dotenv": "16.0.3",
    "errors": "^0.3.0",
    "event-emitter": "^0.3.5",
    "events": "^3.3.0",
    "express": "^4.18.2",
    "express-http-proxy": "^1.6.3",
    "faker": "^5.5.3",
    "fast-myers-diff": "^3.0.1",
    "figlet": "^1.6.0",
    "file-regex": "^3.1.0",
    "file-saver": "^2.0.5",
    "fs-extra": "^11.1.1",
    "fuzzy": "^0.1.3",
    "graphql": "^16.6.0",
    "graphql-request": "^6.1.0",
    "graphql-tag": "^2.12.6",
    "inflection": "^2.0.1",
    "inquirer": "^8.2.4",
    "isomorphic-fetch": "^3.0.0",
    "isomorphic-git": "^1.22.0",
    "jest": "^29.7.0",
    "lodash": "^4.17.21",
    "log-timestamp": "^0.3.0",
    "lorem-ipsum": "^2.0.4",
    "moment": "^2.29.1",
    "node-fetch": "^2.6.9",
    "open": "^9.1.0",
    "rc": "^1.2.8",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.6.1",
    "temp": "^0.9.4",
    "url-join": "^4.0.1",
    "util": "^0.12.4"
  },
  "devDependencies": {
    "@types/archiver": "^5.3.3",
    "@types/debug": "^4.1.9",
    "@types/event-emitter": "^0.3.5",
    "@types/events": "^3.0.3",
    "@types/figlet": "^1.5.8",
    "@types/fs-extra": "^11.0.2",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.10.3",
    "@types/rc": "^1.2.2",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "debug": "^4.3.3",
    "eslint": "^8.53.0",
    "eslint-config-lambdas": "^0.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "mocha": "^9.1.3",
    "prettier": "^3.1.1",
    "rimraf": "^4.4.0",
    "sinon": "^17.0.1",
    "sinon-chai": "^3.7.0",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2"
  },
  "overrides": {
    "lokka-transport-http": {
      "babel-runtime": {
        "core-js": "^3.23.3"
      }
    }
  }
}
