{
  "name": "binocular",
  "version": "v2.0.0",
  "description": "Time-based visualizations of combined software engineering data",
  "main": "binocular.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./types/esm/index.d.ts",
        "default": "./esm/index.js"
      },
      "require": {
        "types": "./types/commonjs/index.d.cts",
        "default": "./commonjs/index.cjs"
      }
    }
  },
  "scripts": {
    "test": "npm run test-server && npm run test-ui",
    "test-server": "mocha --config=.mocharc.json --exit --reporter spec test/backend",
    "test-ui": "jest --config jest.config.js",
    "coverage": "npm run coverage-server && npm run coverage-ui && npm run collect-coverage",
    "coverage-server": "nyc mocha --config=.mocharc.json --exit test/backend",
    "coverage-ui": "jest --config jest-coverage.config.js",
    "collect-coverage": "./scripts/collect-coverage.js",
    "depcheck": "depcheck",
    "eslint:ui": "eslint --ignore-path ui/.eslintignore -c ui/.eslintrc.yml ui",
    "eslint:root": "eslint .",
    "eslint": "npm run eslint:root && npm run eslint:ui",
    "check": "npm run eslint",
    "dev": "binocular run",
    "dev-concurrently": "concurrently --kill-others -P  \"webpack serve --config webpack.dev.js\" \"npm run dev-server {1}\" --",
    "dev-server": "tsx watch binocular.ts run --no-frontend",
    "dev-frontend": "webpack serve --config webpack.dev.js",
    "build:dev": "webpack --config webpack.dev.js --mode=development",
    "build:prod": "webpack --config webpack.prod.js --mode=production",
    "build:offline": "webpack --config webpack.offline.js --mode=production",
    "dockerize": "whalify generate-installer && ./node_modules/.bin/whalify build",
    "fix:root": "eslint --fix .",
    "fix:ui": "eslint --fix --ignore-path ui/.eslintignore -c ui/.eslintrc.yml ui",
    "fix": "npm run fix:root && npm run fix:ui",
    "build:lamosp": "cd services/language/detector && bundler install",
    "start:lamosp": "cd services/language/detector && bundle exec binocular-language-detector"
  },
  "keywords": [
    "git"
  ],
